[
  {
    "name": "rnn_unit",
    "short_desc": "One of two types: [ 'lstm', 'gru' ]",
    "default": "gru",
    "type": "categorical",
    "categorical_vals": ["lstm", "gru"],
    "run_HPO": true
  },
  {
    "name": "embedding_size",
    "short_desc": "Word embedding size",
    "default": 50,
    "type": "int",
    "search_type": "uniform",
    "range_low": 20,
    "range_high": 70,
    "run_HPO": true
  },
  {
    "name": "latent_dim",
    "short_desc": "Latent dim size in RNN unit",
    "default": 20,
    "type": "int",
    "search_type": "uniform",
    "range_low": 10,
    "range_high": 50,
    "run_HPO": true
  },

  {
    "name": "lr",
    "short_desc": "Learning rate for optimizer.",
    "default": 1e-3,
    "type": "real",
    "range_low": 1e-6,
    "range_high": 5e-1,
    "search_type": "log-uniform",
    "run_HPO": true
  }
]
